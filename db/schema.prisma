// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model Excercise {
  id                   String                      @id @default(uuid())
  category             String
  description          String?
  videoURL             String
  defaultDescription   String
  users                User[]
}

model User {
  id        String     @id @default(uuid())
  fullName  String
  email     String     @unique
  password  String
  sumPrice  String?
  packageType String?
  startDate String?
  endDate String?
  sumOfPaidMonths String?
  priceForThisMonth String?
  exercises Excercise[]
}


